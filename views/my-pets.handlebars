{{!-- pet handling --}}
<section class="pet">
    {{!-- <img src="" alt=""> pet image --}}
    {{#each pets as |pet| }}
    <div class="pet-details">
        <h3>My Name is {{pet.pet_name}} </h3>
        <p> About Me: {{pet.about_pet}} </p>
    </div>
    {{/each}}

    {{!-- TEMPORY - CHECK THAT THE USER HAS AUTHENTICATION --}}
    <h1>ID: {{id}}</h1>
    <h1>User ID: {{user_id}}</h1>
    {{> pet-form}}
</section>